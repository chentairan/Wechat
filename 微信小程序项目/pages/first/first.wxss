page
{
  background-color: #f7f7f7;
}

.contain
{
  display:flex;
  flex-direction: row;
  position: fixed;
  top: 0;
  z-index: 10;
  background:-webkit-linear-gradient(top, #53bbbb, rgba(83,187,187,0.9));
  box-shadow: 0 6rpx 8rpx rgba(83,187,187,0.5);
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}
.tab
{
  flex-direction: row;
  height: 80rpx;
  width: 375rpx;
}
.text
{
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
  color: white;
  position: relative;
  top:20rpx;
  left:135rpx;
  font-size: 30rpx;
}

.triangle
{
    width: 0;
    height: 0;
    align-items: center;
    position: relative;
    top:-1rpx;
    left:220rpx;
    border-left: 8rpx solid transparent;
    border-right: 8rpx solid transparent;
    border-top: 14rpx solid rgba(238, 238, 238,0.6);
}
/*按钮样式*/
.button
{
  width: 130rpx;
  height: 130rpx;
  border-radius: 65rpx;
  box-shadow: 1rpx 0rpx 5rpx 1rpx #999999;
  background: #FF9100;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  z-index: 2;
}
/*icon样式*/
.Search-pic
{
  width: 70rpx; 
